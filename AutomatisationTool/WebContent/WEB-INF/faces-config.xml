<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	
	
	<navigation-rule>
		<from-view-id>/pages/champNew.jsp</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/pages/champNew.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>table</from-outcome>
			<to-view-id>/pages/table.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/champEdit.jsp</from-view-id>
	<navigation-case>
			<from-outcome>table</from-outcome>
			<to-view-id>/pages/table.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
		
	<navigation-rule>
		<from-view-id>/pages/testNew.jsp</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionTest.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
		
		<navigation-rule>
		<from-view-id>/pages/testEdit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>table</from-outcome>
			<to-view-id>/pages/tableTests.jsp</to-view-id>
		</navigation-case>
			<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionTest.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
		
		<navigation-rule>
		<from-view-id>/pages/tableTests.jsp</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionTest.jsp</to-view-id>
		</navigation-case>
			<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/pages/testEdit.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
		
	<navigation-rule>
		<from-view-id>/pages/gestionTest.jsp</from-view-id>
	    <navigation-case>
			<from-outcome>table</from-outcome>
			<to-view-id>/pages/tableTests.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/pages/testNew.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>


	<navigation-rule>
		<from-view-id>/pages/success3.jsp</from-view-id>
		<navigation-case>
			<from-outcome>table</from-outcome>
			<to-view-id>/pages/table.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>

   <navigation-rule>
		<from-view-id>/pages/gestionChamp.jsp</from-view-id>
		<navigation-case>
			<from-outcome>table</from-outcome>
			<to-view-id>/pages/table.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/pages/champNew.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
		
		 <navigation-rule>
		<from-view-id>/pages/delete.jsp</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionChamp.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
	
		 <navigation-rule>
		<from-view-id>/pages/champNew.jsp</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionChamp.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
	
		 <navigation-rule>
		<from-view-id>/pages/champDetail.jsp</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionChamp.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
		
			 <navigation-rule>
		<from-view-id>/pages/champEdit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionChamp.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
		
	
	<navigation-rule>
		<from-view-id>/pages/testNew.jsp</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/pages/testNew.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/pages/failure.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/Accueil.jsp</from-view-id>
		<navigation-case>
			<from-outcome>champ</from-outcome>
			<to-view-id>/pages/gestionChamp.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>test</from-outcome>
			<to-view-id>/pages/gestionTest.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/table.jsp</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/pages/champEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>detail</from-outcome>
			<to-view-id>/pages/champDetail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>delete</from-outcome>
			<to-view-id>/pages/delete.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/gestionChamp.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>